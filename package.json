{
  "name": "react-fullstack-node-starter",
  "version": "1.0.0",
  "description": "A simple up and running backend and front end with react and node",
  "main": "server.js",
  "scripts": {
    "webpack-watch": "webpack -w",
    "express-server": "node ./server/server",
    "start": "concurrently --kill-others \"npm run webpack-watch\" \"npm run express-server\"",
    "start-dev": "concurrently --kill-others \"npm run webpack-watch\" \"nodemon ./server/server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siunegu/archaic-monolithic.git"
  },
  "author": "Eugene Lai",
  "license": "MIT",
  "engines": {
    "node": "5.4.1"
  },
  "dependencies": {
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "concurrently": "2.2.0",
    "cookie-parser": "1.4.3",
    "cors": "2.7.1",
    "debug": "2.2.0",
    "express": "4.14.0",
    "express-validation": "1.0.0",
    "http-status": "0.2.3",
    "joi": "8.0.5",
    "jquery": "^3.1.0",
    "lodash": "4.13.1",
    "mongoose": "4.5.4",
    "morgan": "1.7.0",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "react-hot-loader": "1.3.0",
    "webpack": "1.13.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "image-loader": "0.0.1",
    "node-sass": "3.8.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "0.9.1",
    "sass-loader": "4.0.0",
    "source-map-loader": "0.1.5",
